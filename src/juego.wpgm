import wollok.game.*
import frog.*
import obstaculos.*

program juego {
	//CONFIG
	game.title("Frogger")
	game.height(11)
	game.width(9)
	game.boardGround('background2.jpg')
	
	game.addVisualCharacter(frog)
	
	
	game.onCollideDo(frog, { algo => algo.teEncontro(frog)  } )
	
	
	
	game.onTick(3000, 'auto', {generadorVehiculos.generarVehiculo()})

	game.onTick(1000, 'auto', {generadorVehiculos.vehiculosGenerados().forEach {vehiculo => vehiculo.moverse()}})


	//COMIENZA
	game.start()
}