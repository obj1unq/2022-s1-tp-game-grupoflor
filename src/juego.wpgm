import wollok.game.*
import frog.*
import obstaculos.*

program juego {
	//CONFIG
	game.title("Frogger")
	game.height(11)
	game.width(11)
	game.ground('background.png')
	
	game.addVisualCharacter(frog)
	game.addVisualCharacter(car1)
	
	game.onCollideDo(frog, { algo => algo.teEncontro(frog)  } )
	
	
	//COMIENZA
	game.start()
}